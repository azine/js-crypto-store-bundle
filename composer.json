{
    "name": "azine/js-crypto-store-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to store files on the database that have been securely encrypted on the client side (browser).",
    "keywords": ["privacy","storage","file encryption","encryption"],
    "homepage": "https://github.com/azine/js-crypto-store-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Working Web GmbH, Switzerland",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">7.0",
        "symfony/framework-bundle": ">4.4.50|^5.4|^6.3",
        "symfony/console": ">4.4.50|^5.4|^6.3",
        "symfony/finder": ">4.4.50|^5.4|^6.3",
        "symfony/twig-bundle": ">4.4.50|^5.4|^6.3",
        "symfony/yaml": ">4.4.50|^5.4|^6.3",
        "doctrine/orm": "~2.0,>=2.2",
        "monolog/monolog": ">=1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit":  "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-0": { "Azine\\JsCryptoStoreBundle\\": "src" }
    },
    "minimum-stability": "stable",
    "target-dir": "Azine/JsCryptoStoreBundle"

}
