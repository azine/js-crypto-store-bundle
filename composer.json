{
    "name": "azine/js-crypto-store-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to store files on the database that have been securely encrypted on the client side (browser).",
    "keywords": ["privacy","storage","file encryption","encryption"],
    "homepage": "https://github.com/azine/js-crypto-store-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Azine IT Services AG",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "~2.7|^3.0|^4.0",
        "symfony/console": "~2.7|^3.0|^4.0",
        "symfony/finder": "~2.7|^3.0|^4.0",
        "symfony/twig-bundle": "~2.7|^3.0|^4.0",
        "symfony/yaml": "~2.7|^3.0|^4.0",
        "doctrine/orm": "~2.0,>=2.2",
        "monolog/monolog": "~1.6,>=1.6.0"
    },
    "require-dev": {
        "symfony/translation": "~2.7|^3.0|^4.0",
        "phpunit/phpunit":  "~5.7.27|^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "autoload": {
        "psr-0": { "Azine\\JsCryptoStoreBundle\\": "" }
    },
    "minimum-stability": "stable",
    "target-dir": "Azine/JsCryptoStoreBundle"

}
